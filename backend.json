{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "patternProperties": {
    "^[a-zA-Z0-9]{28}$": {
      "type": "object",
      "properties": {
        "age": {
          "type": "string",
          "pattern": "^[0-9]+$"
        },
        "birthday": {
          "type": "string"
        },
        "color": {
          "type": "string"
        },
        "image": {
          "type": "string",
          "format": "uri"
        },
        "name": {
          "type": "string"
        }
      },
      "required": ["age", "birthday", "color", "image", "name"]
    }
  }
}
